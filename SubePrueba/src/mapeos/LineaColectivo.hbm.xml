<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.LineaColectivo" table="lineaColectivo">
		<id column="idLineaColectivo" name="idLineaColectivo" type="long">
			<generator class="identity"/>
		</id>
		<property column="empresa" name="empresa" type="string"/>
		<property column="linea" name="linea" type="int"/>
		<set name = "lstColectivo" cascade = "save-update" table = "colectivo"  inverse = "true" lazy = "true" fetch = "select" >
			<key column = "idLineaColectivo" not-null = "true" />
			<one-to-many class = "datos.Colectivo" />
		</set>
	</class>
</hibernate-mapping>